<h3>Today's new Work Orders</h3>


<div class="thumbnail">

    <div class="caption">
        <table class="table table-hover table-bordered" id="workOrderTableToday">
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Description</th>
                    <th>Status</th>
                    <th>Asset</th>
                    <th>Location</th>
                    <th>Storey</th>
                    <th>Date</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var work in Model.WorkOrders)
                {

                    <tr id="id-@work.Id">

                        <td>
                            @work.Id
                        </td>
                        <td>
                            @work.Description
                        </td>
                        <td>
                            @work.WorkOrderStatus
                        </td>
                        <td>
                            @work.Asset.Label
                        </td>
                        <td>
                            @work.Asset.Space.Label
                        </td>
                        <td>
                            @work.Asset.Space.Storey.Label
                        </td>
                        <td>
                            @work.Date
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
