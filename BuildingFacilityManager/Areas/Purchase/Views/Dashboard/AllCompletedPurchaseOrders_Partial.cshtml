@model BuildingFacilityManager.ViewModels.DashBoardViewModel

<hr />
<br />
<h3>All Completed Purchase Work Orders</h3>

<div class="thumbnail">

    <div class="caption">
        <table class="table table-hover table-bordered" id="CompletedPurchaseToday">
            <thead>
            <tr>
                <th>Id</th>
                <th>Description</th>
                <th>Date</th>
                <th>Cost</th>
            </tr>
            </thead>
            <tbody>
            @foreach (var purchaseOrder in Model.AllCompletedPurchaseOrders)
            {

                <tr id="id-@purchaseOrder.Id">

                    <td>
                        @purchaseOrder.Id
                    </td>
                    <td>
                        @purchaseOrder.Description
                    </td>
                    <td>

                        @purchaseOrder.PurchaseDateTime
                    </td>
                    <td>
                        @purchaseOrder.Cost

                    </td>
                    <td>
                        @purchaseOrder.WorkOrderId
                    </td>
                    
                </tr>
            }
            </tbody>
        </table>
    </div>
</div>
